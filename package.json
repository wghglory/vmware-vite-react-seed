{
  "name": "vmware-vite-react-seed",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "start": "concurrently 'npm:dev' 'npm:mock'",
    "mock": "cd ./src/server && npm start",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "fix": "npx eslint --fix --ext .js,.ts,.tsx .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(vue|js|jsx|ts|tsx|json|css|scss|md|html)\"",
    "postinstall": "cd src/server && yarn"
  },
  "dependencies": {
    "@cds/city": "^1.1.0",
    "@cds/core": "6.0.1",
    "@cds/react": "6.0.1",
    "@singleton-i18n/js-core-sdk": "^0.5.2",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "framer-motion": "^6.3.11",
    "lodash-es": "^4.17.21",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-query": "^3.39.1",
    "react-router-dom": "^6.3.0",
    "react-toastify": "^9.0.4",
    "react-use": "^17.4.0",
    "recoil": "^0.7.3"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.21",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@vitejs/plugin-react": "^1.3.2",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "rollup-plugin-visualizer": "^5.6.0",
    "tailwindcss": "^3.1.2",
    "typescript": "^4.6.3",
    "vite": "^2.9.12",
    "vite-plugin-svgr": "^2.1.0"
  }
